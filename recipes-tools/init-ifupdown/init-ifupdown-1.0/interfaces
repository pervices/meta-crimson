auto eth0
iface eth0 inet static
        address 192.168.10.2
        network 192.168.10.0
        broadcast 192.168.10.255
        netmask 255.255.255.0
post-up ethtool -s eth0 speed 10 duplex half autoneg off
post-up memtool 0xFF803018 0x2202986

