From 5e671028ec03ff723f04692911fea986eee72f10 Mon Sep 17 00:00:00 2001
From: Amir Ghanbari Bavarsad <amir.b@pervices.com>
Date: Thu, 28 Feb 2019 21:35:23 +0000
Subject: [PATCH 5/8] Enabled more modules in the DTS

---
 .../boot/dts/altera/socfpga_stratix10_socdk.dts    | 14 +++++---------
 1 file changed, 5 insertions(+), 9 deletions(-)

diff --git a/arch/arm64/boot/dts/altera/socfpga_stratix10_socdk.dts b/arch/arm64/boot/dts/altera/socfpga_stratix10_socdk.dts
index 7a76d62c5c10..0160691ff98b 100644
--- a/arch/arm64/boot/dts/altera/socfpga_stratix10_socdk.dts
+++ b/arch/arm64/boot/dts/altera/socfpga_stratix10_socdk.dts
@@ -51,17 +51,13 @@
 		clkmgr@ffd10000 {
 			clocks {
 				osc1 {
-					clock-frequency = <125000000>;
+					clock-frequency = <100000000>;
 				};
 			};
 		};
 	};
 };
 
-&gpio1 {
-	status = "disabled";
-};
-
 &gmac0 {
 	phy-mode = "rgmii";
 	phy-addr = <0xffffffff>; /* probe for phy addr */
@@ -84,7 +80,7 @@
 	rxdv-skew-ps = <420>; /* 0ps */
 	rxc-skew-ps = <1680>; /* 780ps */
 	max-frame-size = <3800>;
-	status = "disabled";
+	status = "okay";
 };
 
 &mmc {
@@ -99,13 +95,13 @@
 	status = "okay";
 };
 
-&usb0 {
+&usb1 {
 	status = "disabled";
 	disable-over-current;
 };
 
 &i2c0 {
-	status = "disabled";
+	status = "okay";
 	clock-frequency = <100000>;
 	i2c-sda-falling-time-ns = <300>;  /* hcnt */
 	i2c-sdl-falling-time-ns = <300>;  /* lcnt */
@@ -113,7 +109,7 @@
 };
 
 &i2c2 {
-	status = "disabled";
+	status = "okay";
 	clock-frequency = <100000>;
 	i2c-sda-falling-time-ns = <300>;  /* hcnt */
 	i2c-sdl-falling-time-ns = <300>;  /* lcnt */
-- 
2.20.1

